{
  "name": "uni-query",
  "version": "1.0.0",
  "description": "🐠 为uniapp设计的开源组件库，专注最重要的核心组件",
  "main": "libs/index.js",
  "typings": "./types/index.d.ts",
  "scripts": {
    "init": "npm install && devby --copy ./node_modules/pageby/dist/pageby.min.js ./docs/js/min/pageby.min.js",
    "offiaccount": "offiaccount-cli --config offiaccount.config.js",
    "dev:docs": "devby -s 20000",
    "dev:h5": "uni",
    "dev:mp-weixin": "uni -p mp-weixin",
    "dev:mp-alipay": "uni -p mp-alipay",
    "dev:mp-toutiao": "uni -p mp-toutiao",
    "build:h5": "uni build",
    "build:mp-weixin": "uni build -p mp-weixin",
    "build:mp-alipay": "uni build -p mp-alipay",
    "build:mp-toutiao": "uni build -p mp-toutiao",
    "build:docs": "node ./build/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/oi-contrib/uniQuery.git"
  },
  "author": {
    "name": "zxl20070701",
    "url": "https://zxl20070701.github.io/notebook/home.html"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/oi-contrib/uniQuery/issues"
  },
  "homepage": "https://oi-contrib.github.io/uniQuery",
  "keywords": [
    "uniapp",
    "H5",
    "微信小程序",
    "支付宝小程序",
    "抖音小程序"
  ],
  "browserslist": [
    "Android >= 4.4",
    "ios >= 9"
  ],
  "uni-app": {
    "scripts": {}
  },
  "dependencies": {
    "pointin": "^0.1.1",
    "vislite": "^1.1.0"
  },
  "devDependencies": {
    "@dcloudio/types": "^3.3.2",
    "@dcloudio/uni-app": "3.0.0-3090920231225001",
    "@dcloudio/uni-automator": "3.0.0-3090920231225001",
    "@dcloudio/uni-cli-shared": "3.0.0-3090920231225001",
    "@dcloudio/uni-components": "3.0.0-3090920231225001",
    "@dcloudio/uni-h5": "3.0.0-3090920231225001",
    "@dcloudio/uni-mp-alipay": "3.0.0-3090920231225001",
    "@dcloudio/uni-mp-toutiao": "3.0.0-3090920231225001",
    "@dcloudio/uni-mp-weixin": "3.0.0-3090920231225001",
    "@dcloudio/uni-stacktracey": "3.0.0-3090920231225001",
    "@dcloudio/vite-plugin-uni": "3.0.0-3090920231225001",
    "@vue/runtime-core": "^3.2.45",
    "@vue/tsconfig": "^0.1.3",
    "devby": "^0.7.0",
    "pageby": "^0.1.0",
    "sass": "^1.71.0",
    "typescript": "^4.9.4",
    "vite": "^4.5.3",
    "vue": "^3.2.45",
    "vue-tsc": "^1.0.24",
    "wechat-offiaccount": "^0.2.1",
    "zdebug.js": "^0.3.0"
  }
